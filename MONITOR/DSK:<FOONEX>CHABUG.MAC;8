DEFINE DEFBUG(TYPE,NAME,WHERE,WHAT,MSG,IGNORE,IGNORE)<
DEFINE NAME<
BUG(TYPE,MSG)
>>

DEFINE CHABUG(NAME,IGNORE)<
NAME
>

